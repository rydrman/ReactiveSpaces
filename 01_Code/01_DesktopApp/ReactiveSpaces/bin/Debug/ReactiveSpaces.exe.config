<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  
    <system.serviceModel>
      <bindings>
        <netPeerTcpBinding>
          <binding name="ReactiveSpacesBinding">
            <security mode="None" />
          </binding>
        </netPeerTcpBinding>
      </bindings>
      <services>
        <service name="RSNetworker.Networker">
          <endpoint address="net.p2p://ReactiveSpaces" binding="netPeerTcpBinding" 
                    bindingConfiguration="ReactiveSpacesBinding" name="ReactiveSpaces" contract="RSNetworker.IRSNetworker" />
        </service>
      </services>
      <client>
        <endpoint address="net.p2p://ReactiveSpaces" binding="netPeerTcpBinding"
          bindingConfiguration="ReactiveSpacesBinding" contract="RSNetworker.IRSNetworker"
          name="ReactiveSpacesEndpoint" kind="" endpointConfiguration="">
          <identity>
            <!--<certificateReference storeName="My" storeLocation="LocalMachine"
              x509FindType="FindBySubjectDistinguishedName" />-->
          </identity>
        </endpoint>
      </client>
    </system.serviceModel>

</configuration>